    "# Definindo nosso decoradora\n",
    "def tem_git(funcao):\n",
    "    def escrava():\n",
    "        if os.path.isdir(\".git\"):\n",
    "            funcao()\n",
    "        else:\n",
    "            pass\n",
    "    return escrava()"
   ]
